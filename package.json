{
	"name": "samba-toggle-gnome-shell-extension",
	"private": true,
	"type": "module",
	"main": "src/index.ts",
	"scripts": {
		"build": "tsup-node",
		"check": "prettier --check . && biome check . && tsc --noEmit",
		"fix": "prettier --write . && biome check --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"@girs/gjs": "4.0.0-beta.38",
		"@girs/gnome-shell": "^49.0.1",
		"@types/node": "^24.6.2",
		"prettier": "^3.6.2",
		"tinyexec": "^1.0.1",
		"tsup": "^8.5.0",
		"typescript": "^5.9.3"
	},
	"packageManager": "pnpm@10.12.4",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"esbuild"
		]
	}
}
